Host hosting
    HostName www.johntobin.ie
    ForwardAgent yes
    ForwardX11 no

Host hosting-proxy
    HostName www.johntobin.ie
    ForwardAgent yes
    ForwardX11 no

    # SOCKS proxy
    DynamicForward 10080

# Shorter than typing arianetobin@hosting
Host ar-hosting
    HostName www.johntobin.ie
    ForwardAgent yes
    ForwardX11 no
    User arianetobin

